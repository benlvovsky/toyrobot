using namespace std;

bool ToyRobot::IsPlaced() {
  return this->placed;
}


void ToyRobot::Place(int x, int y) {
	if (x < 0 || x > 4) {
		throw invalid_argument("Position is beyond acceptable");
	}
	this->x = x;
	this->y = y;

}

