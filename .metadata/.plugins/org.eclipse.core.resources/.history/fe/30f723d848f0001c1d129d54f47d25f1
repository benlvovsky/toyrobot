enum Facing {
	NORTH, SOUTH, EAST, WEST
};

class Report {
  private:
    bool placed;

  public:
    Report();
	Facing FACED();
	int X();
	int Y();
};


class ToyRobot {
  private:
    int facing;
    bool placed;

  public:
    ToyRobot() : facing(0), placed(false) {}
	bool IsPlaced();
	void Place(int X, int Y);
	Report Report();
	Facing FACED();
	int X();
	int Y();
};
